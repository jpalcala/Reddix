{
  "name": "reddix",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron ./src/electron.dev",
    "electron-dev": "concurrently \"ng serve\" \"npm run electron\" ",
    "electron-build": "ng build --prod && electron .",
    "package:win": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite ",
    "package:linux": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite ",
    "package:osx": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite ",
    "package:all": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0-rc.0",
    "@angular/common": "~7.0.0-rc.0",
    "@angular/compiler": "~7.0.0-rc.0",
    "@angular/core": "~7.0.0-rc.0",
    "@angular/forms": "~7.0.0-rc.0",
    "@angular/http": "~7.0.0-rc.0",
    "@angular/platform-browser": "~7.0.0-rc.0",
    "@angular/platform-browser-dynamic": "~7.0.0-rc.0",
    "@angular/router": "~7.0.0-rc.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.9.0-rc.1",
    "@angular/cli": "~7.0.0-rc.1",
    "@angular/compiler-cli": "~7.0.0-rc.0",
    "@angular/language-service": "~7.0.0-rc.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "concurrently": "^4.0.1",
    "cross-var": "^1.1.0",
    "electron": "^3.0.2",
    "electron-packager": "^12.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
